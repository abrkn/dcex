{
  "name": "with-apollo",
  "version": "2.0.0",
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "dev:watch": "NODE_ENV=development nodemon -x 'node server.js'",
    "heroku-postbuild": "next build",
    "build": "next build",
    "start": "node server.js",
    "db:drop": "psql postgres://$DATABASE_HOST -c \"drop database $DB_NAME\" postgres",
    "db:drop:if-exists": "run-s -sl db:drop || true",
    "db:create": "psql postgres://$DATABASE_HOST -c \"create database $DB_NAME\" postgres",
    "db:schema": "psql $DATABASE_URL --single-transaction -v ON_ERROR_STOP=on -f schema.sql $DB_NAME",
    "db:disconnect": "psql $DATABASE_URL --single-transaction -v ON_ERROR_STOP=on -f disconnect.sql $DB_NAME || true",
    "db:bootstrap": "run-s db:disconnect db:drop:if-exists db:create db:schema",
    "db:bootstrap:watch": "nodemon -e sql -x 'run-s db:bootstrap'"
  },
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "bitcoin": "3.0.1",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-graphql": "0.6.12",
    "graphql": "14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "4.17.11",
    "next": "7.0.2",
    "npm-run-all": "4.1.3",
    "p-map": "2.0.0",
    "panik": "1.0.1",
    "pg-promise": "8.5.0",
    "prop-types": "^15.6.2",
    "react": "16.5.2",
    "react-apollo": "^2.1.11",
    "react-dom": "16.5.2",
    "safep": "1.2.6"
  },
  "engines": {
    "node": ">=9"
  },
  "author": "",
  "license": "ISC"
}
